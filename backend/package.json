{
  "name": "anchor-float-backend",
  "version": "1.0.0",
  "description": "The backend of Anchor Float",
  "scripts": {
    "start": "nodemon -e js,graphql -x node api/index.js",
    "dev": "nodemon -e js,graphql -x node --inspect api/index.js",
    "devscan": "nodemon -e js,graphql -x node --inspect scan/index.js",
    "test": "jest --watchAll",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "devdb": "prisma2 dev",
    "postinstall": "prisma2 generate",
    "clean": "rm -rf node_modules && rm -rf ./prisma/migrations && rm -rf ./prisma/dev.db && npm i",
    "devdb:up": "prisma2 lift save && prisma2 lift up",
    "scanner": "nodemon ./scan/index.js",
    "sitemap": "node ./scan/cmd.js",
    "seed": "node ./prisma/seed.js",
    "purge-parse-sitemap": "aws sqs purge-queue --queue-url https://sqs.us-east-1.amazonaws.com/510755279176/parse-sitemap",
    "purge-parse-page": "aws sqs purge-queue --queue-url https://sqs.us-east-1.amazonaws.com/510755279176/parse-page",
    "purge-parse-shortlink": "aws sqs purge-queue --queue-url https://sqs.us-east-1.amazonaws.com/510755279176/parse-shortlink",
    "purge-queue": "purge-parse-sitemap && purge-parse-page && purge-parse-shortlink"
  },
  "dependencies": {
    "apollo-server-express": "^2.8.1",
    "axios": "^0.18.1",
    "bcryptjs": "2.4.3",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.4",
    "crypto": "^1.0.1",
    "cuid": "^2.1.6",
    "dotenv": "^6.0.0",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.3.0",
    "nodemon": "^1.19.1",
    "sitemapper": "^2.2.0",
    "sqs-consumer": "^5.3.0",
    "sqs-producer": "^1.6.3",
    "util": "^0.12.1"
  },
  "license": "MIT",
  "repository": {},
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-ignore-generated-and-nolint": "^1.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "jest": "^23.6.0",
    "prisma2": "^2.0.0-preview013.3"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "semi": true,
    "trailingComma": "es5"
  },
  "jest": {
    "verbose": true
  }
}
