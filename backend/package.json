{
  "name": "anchor-float-backend",
  "version": "1.0.0",
  "description": "The backend of Anchor Float",
  "scripts": {
    "start": "nodemon -e js,graphql -x node api/index.js",
    "dev": "nodemon -e js,graphql -x node --inspect api/index.js",
    "devscan": "nodemon -e js,graphql -x node --inspect scan/index.js",
    "devreport": "nodemon -e js,graphql -x node --inspect report/index.js",
    "test": "jest --watchAll",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf node_modules && rm -rf ./prisma/migrations && npm i",
    "scanner": "nodemon ./scan/index.js",
    "sitemap": "node ./scan/cmd.js",
    "report": "node ./report/cmd.js",
    "seed": "node ./prisma/seed.js",
    "generate": "prisma2 generate",
    "purge-parse-sitemap": "aws sqs purge-queue --queue-url https://sqs.us-east-1.amazonaws.com/510755279176/parse-sitemap",
    "purge-parse-page": "aws sqs purge-queue --queue-url https://sqs.us-east-1.amazonaws.com/510755279176/parse-page",
    "purge-parse-shortlink": "aws sqs purge-queue --queue-url https://sqs.us-east-1.amazonaws.com/510755279176/parse-shortlink",
    "purge-parse-product": "aws sqs purge-queue --queue-url https://sqs.us-east-1.amazonaws.com/510755279176/parse-product",
    "purge-generate-report": "aws sqs purge-queue --queue-url https://sqs.us-east-1.amazonaws.com/510755279176/generate-report",
    "purge-queue": "npm run purge-parse-sitemap && npm run purge-parse-page && npm run purge-parse-shortlink && npm run purge-parse-product && npm run purge-generate-report"
  },
  "dependencies": {
    "@google-cloud/storage": "^4.1.0",
    "apollo-server-express": "^2.8.1",
    "axios": "^0.18.1",
    "bcryptjs": "2.4.3",
    "bottleneck": "^2.19.5",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.4",
    "crypto": "^1.0.1",
    "crypto-js": "^3.1.9-1",
    "cuid": "^2.1.6",
    "dotenv": "^6.0.0",
    "express": "^4.17.1",
    "googleapis": "^44.0.0",
    "graphql": "^14.4.2",
    "json2csv": "^4.5.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.3.0",
    "nodemon": "^1.19.1",
    "path": "^0.12.7",
    "sitemapper": "^2.2.0",
    "sqs-consumer": "^5.3.0",
    "sqs-producer": "^1.6.3",
    "stripe": "^7.10.0",
    "superagent": "3.5.2",
    "util": "^0.12.1",
    "uuid": "^3.3.3",
    "xml2js": "^0.4.22"
  },
  "license": "MIT",
  "repository": {},
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-ignore-generated-and-nolint": "^1.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "jest": "^23.6.0",
    "prisma2": "^2.0.0-preview015",
    "mocha": "~2.3.4",
    "sinon": "1.17.3",
    "expect.js": "~0.3.1"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "semi": true,
    "trailingComma": "es5"
  },
  "jest": {
    "verbose": true
  }
}
