type User {
  id: ID! @id @unique
  name: String!
  email: String!
  password: String!
  domains: [Domain]! @relation(link: INLINE)
}

type Domain {
  id: ID! @id @unique
  pages: [Page]! @relation(link: INLINE)
  url: String!
}

type Page {
  id: ID! @id @unique
  url: String!
  pageName: String!
  links: [Link]! @relation(link: INLINE)
}

type Link {
  id: ID! @id @unique
  url: String!
  affiliateTagged: Boolean!
  affiliateTagName: String
  product: Product @relation(link: INLINE)
}

type Product {
  id: ID! @id @unique
  name: String!
  asin: String!
}
