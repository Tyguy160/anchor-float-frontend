type User {
  id: ID! @id @unique
  name: String!
  email: String!
  password: String!
  domains: [Domain]!
}

type Domain {
  id: ID! @id @unique
  pages: [Page]!
  url: String!
}

type Page {
  id: ID! @id @unique
  url: String!
  pageName: String!
  links: [Link]! 
}

type Link {
  id: ID! @id @unique
  url: String!
  affiliateTagged: Boolean!
  affiliateTagName: String
  product: Product 
}

type Product {
  id: ID! @id @unique
  name: String!
  asin: String!
}
