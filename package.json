    "date-fns": "^2.0.0-beta.3",
    "downshift": "^2.2.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "formik": "^2.0.4",
    "glamor": "^2.20.40",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.9.2",
